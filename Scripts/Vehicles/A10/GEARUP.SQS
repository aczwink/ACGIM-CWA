_plane = _this select 0
_plane animate ["CanGear", 1]
~1
_plane say "Gear";
_plane animate ["LeftGear", 1]
_plane animate ["RightGear", 1]
_plane animate ["NoseGear", 1]
_plane animate ["NoseGearDoor", 1]
_plane animate ["LeftGearDoor", 1]
_plane animate ["RightGearDoor", 1]
~2
_plane animate ["GearW", 1]
_plane animate ["NoseGearDoor2", 0]
exit
